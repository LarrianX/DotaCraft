{
  "schemaVersion": 1,
  "id": "dotacraft",
  "version": "${version}",
  "name": "DotaCraft",
  "description": "A mod for Minecraft that transforms it into Dota 2.",
  "authors": [
    "LarrianX",
    "Youlday",
    "Nuklon"
  ],
  "contact": {
    "sources": "https://github.com/LarrianX/DotaCraft"
  },
  "license": "MIT",
  "icon": "assets/dotacraft/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.larrian.dotacraft.DotaCraft"
    ],
    "client": [
      "com.larrian.dotacraft.client.DotaCraftClient"
    ],
    "cardinal-components": [
      "com.larrian.dotacraft.ModComponents"
    ]
  },
  "mixins": [
    "dotacraft.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "fabric-api": "*",
    "cardinal-components-base": ">=5.2.2",
    "cardinal-components-entity": ">=5.2.2"
  },
  "suggests": {
  },
  "custom": {
    "cardinal-components": [
      "dotacraft:hero",
      "dotacraft:attributes"
    ]
  }
}