{
  "schemaVersion": 1,
  "id": "dotacraft",
  "version": "${version}",
  "name": "DotaCraft",
  "description": "Мод переносящий Dota 2 в Minecraft",
  "authors": [
    "LarrianX - code",
    "MrTime4ik - code",
    "Youlday - idea",
    "Nuklon - sprite artist"
  ],
  "contact": {
    "sources": "https://github.com/LarrianX/DotaCraft"
  },
  "license": "MIT",
  "icon": "assets/dotacraft/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.dota2.DotaCraft"
    ],
    "client": [
      "com.dota2.DotaCraftClient"
    ],
    "cardinal-components": [
      "com.dota2.component.ModComponents"
    ]
  },
  "mixins": [
    "dotacraft.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "fabric-api": "*"
  },
  "suggests": {
  },
  "custom": {
    "cardinal-components": [
      "dotacraft:effect",
      "dotacraft:hero",
      "dotacraft:values",
      "dotacraft:max_values",
      "dotacraft:old_values"
    ]
  }
}